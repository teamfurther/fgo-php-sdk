{
  "name": "teamfurther/fgo-php-sdk",
  "description": "A PHP SDK wrapper for fgo.ro API.",
  "keywords": [
    "further",
    "furtherdigitalsolutions",
    "fgo"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Further Digital Solutions",
      "email": "hello@gofurther.digital"
    }
  ],
  "require": {
    "php": ">=8.0",
    "guzzlehttp/guzzle": "^7.8"
  },
  "autoload": {
    "psr-4": {
      "FGO\\": "src"
    }
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
